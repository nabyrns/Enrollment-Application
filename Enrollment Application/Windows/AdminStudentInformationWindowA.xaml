<Window x:Class="Enrollment_Application.AdminStudentInformationWindowA"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enrollment_Application"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        MouseLeftButtonDown="Window_MouseLeftButtonDown"
        FontFamily="Segoe UI"
        Title="Student Information Page" FontSize="14" BorderBrush="#2196f3" BorderThickness="1" Height="720" Width="1130" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None">

    <Grid Height="720" Width="1080" HorizontalAlignment="Center">

        <Button x:Name="closeBtn" HorizontalAlignment="Right" Background="Transparent" BorderBrush="Transparent" VerticalAlignment="Top" Click="CloseBtn_Click">
            <materialDesign:PackIcon Kind="Close" Foreground="Red" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Button>

        <StackPanel Orientation="Vertical" >
            <TextBlock Text="Student Information" Foreground="#2196f3" FontSize="24" HorizontalAlignment="Center" Margin="10"/>

            <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" Height="660" >
                <StackPanel Orientation="Vertical" Margin="10">
                    <TextBlock Foreground="#2196f3" Text="Basic Information" FontSize="24"  HorizontalAlignment="Left"  />
                    <StackPanel x:Name="BasicInfoField">
                        <StackPanel Orientation="Horizontal" x:Name="BasicInfoNameField">
                            <TextBlock Text="First Name: " VerticalAlignment="Center"  />
                            <TextBox Text="{Binding firstName, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aFirstName"  Margin="10" Width="150"  />

                            <TextBlock Text="MI: " VerticalAlignment="Center"  Margin="10,0,0,0" />
                            <TextBox Text="{Binding middleInitial, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aMiddleInitial"  Margin="10" Width="50"  />

                            <TextBlock Text="Lastname: " VerticalAlignment="Center"  Margin="10,0,0,0" />
                            <TextBox Text="{Binding lastName, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aLastName"  Margin="10" Width="150"  />


                        </StackPanel>
                        <StackPanel Orientation="Horizontal" x:Name="BasicInfoEducationField">
                            <TextBlock Text="Current Education Level:  " VerticalAlignment="Center"  Margin="0,0,0,0" />

                            <ComboBox Text="{Binding currentEdLevel, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aEducationLevel" Width="100" Margin="10" VerticalAlignment="Top">
                                <ComboBoxItem Content="Some High School"/>
                                <ComboBoxItem Content="High School Graduate"/>
                                <ComboBoxItem Content="GED"/>
                                <ComboBoxItem Content="Some College"/>
                                <ComboBoxItem Content="Associate's Degree"/>
                                <ComboBoxItem Content="Bachelor's Degree"/>
                            </ComboBox>

                            <TextBlock Text="Sending High School:  " VerticalAlignment="Center"  Margin="10,0,0,0" />

                            <ComboBox Text="{Binding sendingHS, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aSendingHighSchool" Width="200" Margin="10" VerticalAlignment="Top">
                                <ComboBoxItem Content="Yukon High School"/>
                                <ComboBoxItem Content="Mustang High School"/>
                            </ComboBox>

                            <TextBlock Text="Program:  " VerticalAlignment="Center"  Margin="10,0,0,0" />

                            <ComboBox Text="{Binding program, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aProgram" Width="200" Margin="10" VerticalAlignment="Top" >
                                <ComboBoxItem Content="Accounting and Business Education" />
                                <ComboBoxItem Content="Auto Collision Technology" />
                                <ComboBoxItem Content="Auto Service Technology" />
                                <ComboBoxItem Content="Aviation Maintenance Technology" />
                                <ComboBoxItem Content="Biomedical Sciences" />
                                <ComboBoxItem Content="Business Office Technology" />
                                <ComboBoxItem Content="Computer Aided Drafting and Design" />
                                <ComboBoxItem Content="Computer Information Systems" />
                                <ComboBoxItem Content="Computer Programming" />
                                <ComboBoxItem Content="Construction Trades" />
                                <ComboBoxItem Content="Cosmetology" />
                                <ComboBoxItem Content="Diesel Technology" />
                                <ComboBoxItem Content="Digital Media Technology" />
                                <ComboBoxItem Content="Early Care and Education" />
                                <ComboBoxItem Content="Electrical Trades" />
                                <ComboBoxItem Content="Graphic Design" />
                                <ComboBoxItem Content="Health Careers" />
                                <ComboBoxItem Content="Heating, Ventilation, Air Conditioning and Refrigeration" />
                                <ComboBoxItem Content="Medical Assistant" />
                                <ComboBoxItem Content="Medical Coding Specialist" />
                                <ComboBoxItem Content="Medical Office Assistant" />
                                <ComboBoxItem Content="Powersports" />
                                <ComboBoxItem Content="Practical Nursing" />
                                <ComboBoxItem Content="Pre-Engineering" />
                                <ComboBoxItem Content="Precision Machining Technology" />
                                <ComboBoxItem Content="Project Search" />
                                <ComboBoxItem Content="Service Careers" />
                                <ComboBoxItem Content="Service Careers - Hospitality" />
                                <ComboBoxItem Content="Surgical Technology" />
                                <ComboBoxItem Content="Welding" />
                            </ComboBox>

                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                            <TextBlock Text="Attended a college or technology center: " Foreground="GhostWhite" Margin="0, 10, 0, 0"/>
                            <ComboBox ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" Name="attendedCollegeCombo" Width="100" Margin="10, 0, 10, 0" VerticalAlignment="Top" Text="{Binding attendedCollegeOrTech, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}">
                                <ComboBoxItem Content="Yes"/>
                                <ComboBoxItem Content="No"/>
                            </ComboBox>

                            <TextBlock Text="Lives with parent/guardian:" Foreground="GhostWhite" Margin="0, 10, 0, 0"/>
                            <ComboBox ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" Name="liveWithParentCombo" Width="100" Margin="10, 0, 10, 0" VerticalAlignment="Top" Text="{Binding liveWithParent, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}">
                                <ComboBoxItem Content="Yes"/>
                                <ComboBoxItem Content="No"/>
                            </ComboBox>
                        </StackPanel>


                        <StackPanel Orientation="Horizontal" x:Name="BasicInfoContactField">
                            <TextBlock Text="Primary Phone: "  VerticalAlignment="Center" />
                            <TextBox Text="{Binding primaryPhoneNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentPrimaryPhone"   Width="200"  Margin="10" />

                            <TextBlock Text="Cell Phone: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox Text="{Binding cellPhoneNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentCellPhone"   Width="200"  Margin="10" />
                            
                            <TextBlock Text="Social Security Number: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox Text="{Binding cellPhoneNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aSocialSecurity"   Width="200"  Margin="10" />


                        </StackPanel>

                        <Grid Margin="0,10,0,0" x:Name="BasicInfoAddressDemoField">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <StackPanel Orientation="Vertical" Grid.Column="0" >

                                <TextBlock Foreground="#2196f3" Text="Student Address" FontSize="20"  HorizontalAlignment="Left" />


                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="Street Address: "  VerticalAlignment="Center" />
                                    <TextBox Text="{Binding streetAddress, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentStreetAddress"   Width="300"  Margin="10" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="City: "  VerticalAlignment="Center" />
                                    <TextBox Text="{Binding city, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentCity"   Width="150"  Margin="10" />

                                    <TextBlock Text="State: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                                    <ComboBox Text="{Binding state, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentState" Width="50" Margin="10">
                                        <ComboBoxItem Content="AK"/>
                                        <ComboBoxItem Content="AL"/>
                                        <ComboBoxItem Content="AR"/>
                                        <ComboBoxItem Content="AZ"/>
                                        <ComboBoxItem Content="CA"/>
                                        <ComboBoxItem Content="CO"/>
                                        <ComboBoxItem Content="CT"/>
                                        <ComboBoxItem Content="DE"/>
                                        <ComboBoxItem Content="FL"/>
                                        <ComboBoxItem Content="GA"/>
                                        <ComboBoxItem Content="HI"/>
                                        <ComboBoxItem Content="IA"/>
                                        <ComboBoxItem Content="ID"/>
                                        <ComboBoxItem Content="IL"/>
                                        <ComboBoxItem Content="IN"/>
                                        <ComboBoxItem Content="KS"/>
                                        <ComboBoxItem Content="KY"/>
                                        <ComboBoxItem Content="LA"/>
                                        <ComboBoxItem Content="MA"/>
                                        <ComboBoxItem Content="MD"/>
                                        <ComboBoxItem Content="ME"/>
                                        <ComboBoxItem Content="MI"/>
                                        <ComboBoxItem Content="MN"/>
                                        <ComboBoxItem Content="MO"/>
                                        <ComboBoxItem Content="MS"/>
                                        <ComboBoxItem Content="MT"/>
                                        <ComboBoxItem Content="NC"/>
                                        <ComboBoxItem Content="ND"/>
                                        <ComboBoxItem Content="NE"/>
                                        <ComboBoxItem Content="NH"/>
                                        <ComboBoxItem Content="NJ"/>
                                        <ComboBoxItem Content="NM"/>
                                        <ComboBoxItem Content="NV"/>
                                        <ComboBoxItem Content="NY"/>
                                        <ComboBoxItem Content="OH"/>
                                        <ComboBoxItem Content="OK"/>
                                        <ComboBoxItem Content="OR"/>
                                        <ComboBoxItem Content="PA"/>
                                        <ComboBoxItem Content="RI"/>
                                        <ComboBoxItem Content="SC"/>
                                        <ComboBoxItem Content="SD"/>
                                        <ComboBoxItem Content="TN"/>
                                        <ComboBoxItem Content="TX"/>
                                        <ComboBoxItem Content="UT"/>
                                        <ComboBoxItem Content="VA"/>
                                        <ComboBoxItem Content="VT"/>
                                        <ComboBoxItem Content="WA"/>
                                        <ComboBoxItem Content="WI"/>
                                        <ComboBoxItem Content="WV"/>
                                        <ComboBoxItem Content="WY"/>

                                    </ComboBox>

                                    <TextBlock Text="ZIP: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                                    <TextBox Text="{Binding zipCode, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentZIP"   Width="75"  Margin="10"/>
                                </StackPanel>


                            </StackPanel>
                            <StackPanel Orientation="Vertical" Grid.Column="2" >

                                <TextBlock Foreground="#2196f3" Text="Student Demographics" FontSize="20"  HorizontalAlignment="Left" />


                                <StackPanel Orientation="Horizontal">
                                    <TextBlock  Text="Hispanic/Latino" Foreground="GhostWhite" Margin="0,10,0,15" VerticalAlignment="Center"/>
                                    <ComboBox Text="{Binding hispanicOrLatino, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentLatino" Width="100" Margin="10" VerticalAlignment="Top">
                                        <ComboBoxItem Content="Yes"/>
                                        <ComboBoxItem Content="No"/>
                                    </ComboBox>
                                    <TextBlock  Text="Race:" Foreground="GhostWhite" Margin="0,10,0,15" VerticalAlignment="Center"/>
                                    <ComboBox Text="{Binding race, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentRace" Width="100" Margin="10" VerticalAlignment="Top">
                                        <ComboBoxItem Content="American Indian or Alaskan Native"/>
                                        <ComboBoxItem Content="Asian"/>
                                        <ComboBoxItem Content="Black or African American"/>
                                        <ComboBoxItem Content="Hawaiian or Pacific Islander"/>
                                        <ComboBoxItem Content="White"/>
                                    </ComboBox>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock  Text="Gender: " Foreground="GhostWhite" Margin="0,10,0,15" VerticalAlignment="Center"/>
                                    <ComboBox Text="{Binding gender, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentGender" Width="50" Margin="10" VerticalAlignment="Top">
                                        <ComboBoxItem Content="Male"/>
                                        <ComboBoxItem Content="Female"/>
                                    </ComboBox>
                                    <TextBlock  Text="Date of Birth:" Foreground="GhostWhite" Margin="0,10,0,15" VerticalAlignment="Center"/>

                                    <DatePicker Text="{Binding dateOfBirth, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="aStudentDOB" Width="100" Margin="10"/>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </StackPanel>



                    <TextBlock Foreground="#2196f3" Text="Emergency Contact Information" FontSize="24"  HorizontalAlignment="Left" Margin="0,20"  />

                    <Grid x:Name="EmergencyContactField">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>



                        <StackPanel Orientation="Vertical" Grid.Column="0" Grid.Row="0">
                            <TextBlock Foreground="#2196f3" Text="Nearest Relative" FontSize="20"  HorizontalAlignment="Left" />
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Name: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding nameNearestRelative, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRName"   Width="300"  Margin="10" />



                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Relationship: "  VerticalAlignment="Center"  />
                                <TextBox Text="{Binding NRrelationship, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRRelationship"  Width="150"  Margin="10" />


                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Street Address: "  VerticalAlignment="Center"  />
                                <TextBox Text="{Binding NRstreetAddress, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRStreetAddress"  Width="300"  Margin="10" />


                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Primary Number: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding NRprimaryNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRPrimaryNumber"  Width="150"  Margin="10" />


                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Cell Number: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding NRcellNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRAlternateNumber"  Width="150"  Margin="10" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" >
                                <TextBlock Text="City: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding NRcity, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRcity"   Width="150"  Margin="10" />

                                <TextBlock Text="State: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                                <ComboBox Text="{Binding NRstate, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRstate" Width="50" Margin="10">
                                    <ComboBoxItem Content="AK"/>
                                    <ComboBoxItem Content="AL"/>
                                    <ComboBoxItem Content="AR"/>
                                    <ComboBoxItem Content="AZ"/>
                                    <ComboBoxItem Content="CA"/>
                                    <ComboBoxItem Content="CO"/>
                                    <ComboBoxItem Content="CT"/>
                                    <ComboBoxItem Content="DE"/>
                                    <ComboBoxItem Content="FL"/>
                                    <ComboBoxItem Content="GA"/>
                                    <ComboBoxItem Content="HI"/>
                                    <ComboBoxItem Content="IA"/>
                                    <ComboBoxItem Content="ID"/>
                                    <ComboBoxItem Content="IL"/>
                                    <ComboBoxItem Content="IN"/>
                                    <ComboBoxItem Content="KS"/>
                                    <ComboBoxItem Content="KY"/>
                                    <ComboBoxItem Content="LA"/>
                                    <ComboBoxItem Content="MA"/>
                                    <ComboBoxItem Content="MD"/>
                                    <ComboBoxItem Content="ME"/>
                                    <ComboBoxItem Content="MI"/>
                                    <ComboBoxItem Content="MN"/>
                                    <ComboBoxItem Content="MO"/>
                                    <ComboBoxItem Content="MS"/>
                                    <ComboBoxItem Content="MT"/>
                                    <ComboBoxItem Content="NC"/>
                                    <ComboBoxItem Content="ND"/>
                                    <ComboBoxItem Content="NE"/>
                                    <ComboBoxItem Content="NH"/>
                                    <ComboBoxItem Content="NJ"/>
                                    <ComboBoxItem Content="NM"/>
                                    <ComboBoxItem Content="NV"/>
                                    <ComboBoxItem Content="NY"/>
                                    <ComboBoxItem Content="OH"/>
                                    <ComboBoxItem Content="OK"/>
                                    <ComboBoxItem Content="OR"/>
                                    <ComboBoxItem Content="PA"/>
                                    <ComboBoxItem Content="RI"/>
                                    <ComboBoxItem Content="SC"/>
                                    <ComboBoxItem Content="SD"/>
                                    <ComboBoxItem Content="TN"/>
                                    <ComboBoxItem Content="TX"/>
                                    <ComboBoxItem Content="UT"/>
                                    <ComboBoxItem Content="VA"/>
                                    <ComboBoxItem Content="VT"/>
                                    <ComboBoxItem Content="WA"/>
                                    <ComboBoxItem Content="WI"/>
                                    <ComboBoxItem Content="WV"/>
                                    <ComboBoxItem Content="WY"/>

                                </ComboBox>

                                <TextBlock Text="ZIP: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                                <TextBox Text="{Binding NRzip, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="NRzip"   Width="75"  Margin="10"/>

                            </StackPanel>
                           
                        </StackPanel>



            

                        <StackPanel Orientation="Vertical" Grid.Column="2" Grid.Row="0" Margin="0,5,0,0">
                            <TextBlock Foreground="#2196f3" Text="Emergency Contact" FontSize="20"  HorizontalAlignment="Left" />
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Name: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding EContactName, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="ECName"   Width="300"  Margin="10" />

                                

                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Relationship: "  VerticalAlignment="Center"  />
                                <TextBox Text="{Binding EContactRelationship, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="ECRelationship"  Width="150"  Margin="10" />


                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Primary Number: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding EContactPrimaryNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="ECPrimaryNum"  Width="150"  Margin="10" />

                                
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Cell Number: "  VerticalAlignment="Center" />
                                <TextBox Text="{Binding EContactCellNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" x:Name="ECCellNum"  Width="150"  Margin="10" />
                            </StackPanel>
                        </StackPanel>

                    </Grid>



                    <StackPanel x:Name="HealthInformationField">
                        <TextBlock Foreground="#2196f3" Text="Health Information" FontSize="24"  HorizontalAlignment="Left" Margin="0,20"  />

                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Primary Physician: "  VerticalAlignment="Center" />
                            <TextBox x:Name="primaryPhysicianText" Text="{Binding primaryPhysician, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="300"  Margin="10" />

                            <TextBlock Text="Phone Number: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox x:Name="primaryPhysPhoneText" Text="{Binding pPhysicianPhoneNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="150"  Margin="10" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Other Physician: "  VerticalAlignment="Center" />
                            <TextBox x:Name="oPhysicianText" Text="{Binding otherPhysician, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="300"  Margin="10" />

                            <TextBlock Text="Phone Number: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox x:Name="oPhysicianPhoneNum" Text="{Binding oPhysicianPhoneNum, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="150"  Margin="10" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" >
                            <TextBlock Text="Diabetes:" Foreground="GhostWhite"  VerticalAlignment="Center"/>
                            <ComboBox x:Name="diabetesCombo" Text="{Binding diabeticType, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" Width="100" Margin="10">
                                <ComboBoxItem Content="None"/>
                                <ComboBoxItem Content="Type I"/>
                                <ComboBoxItem Content="Type II"/>
                            </ComboBox>

                            <TextBlock Text="Allergies: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox x:Name="allergiesText" Text="{Binding allergies, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="300"  Margin="10" />

                            <TextBlock Text="Heart Conditions: "  VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox x:Name="heartConditionsText" Text="{Binding heartIssues, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}"  Width="300"  Margin="10" />

                        </StackPanel>

                        <TextBlock Text="Relevant Condition(s):" Foreground="GhostWhite"  VerticalAlignment="Center" Margin="0,10"/>
                        <StackPanel Orientation="Horizontal" >


                            <CheckBox x:Name="metabolicCheck" IsChecked="{Binding metabolic, ValidatesOnDataErrors=True, UpdateSourceTrigger=PropertyChanged}" ToolTip="{Binding Path=(Validation.Errors).CurrentItem.ErrorContent, RelativeSource={x:Static RelativeSource.Self}}" Validation.ErrorTemplate="{x:Null}" HorizontalAlignment="Center" Margin="10,0,0,0" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Center">
                                <CheckBox.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1.5" ScaleY="1.5"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
